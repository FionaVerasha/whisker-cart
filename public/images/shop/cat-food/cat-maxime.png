<script>
jQuery(window).ready(function(){

    function sticky_class() {
        if (jQuery(this).scrollTop() > 50) {
            jQuery("body").addClass("scrolled");
        } else {
            jQuery("body").removeClass("scrolled");
        }
    }
    jQuery(window).scroll(function(){
        sticky_class();
    });
    sticky_class();

    jQuery('.maxime-hp-review-text-wrapper').slick({
        slidesToShow: 1,
        slidesToScroll: 1,
        arrows: false,
        fade: true,
        asNavFor: '.maxime-hp-review-thumb-wrapper',
        responsive: [
            {
                breakpoint: 981,
                settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1,
                    arrows: false,
                    fade: true,
                    asNavFor: '.maxime-hp-review-thumb-wrapper',
                    autoplay: true,
                    autoplaySpeed: 10000,
                }
            }
        ]
    });

    jQuery('.maxime-hp-review-thumb-wrapper').slick({
        slidesToShow: 3,
        slidesToScroll: 1,
        asNavFor: '.maxime-hp-review-text-wrapper',
        centerMode: true,
        arrows: false,
        focusOnSelect: true,
        responsive: [
            {
                breakpoint: 981,
                settings: {
                    arrows: false,
                    centerMode: false,
                    asNavFor: '.maxime-hp-review-text-wrapper',
                    slidesToShow: 1,
                    autoplay: true,
                    autoplaySpeed: 10000,
                }
            }
        ]
    });

    jQuery('.footer-products-wrapper').slick({
        slidesToShow: 4,
        slidesToScroll: 1,
        arrows: true,
        autoplay: true,
        autoplaySpeed: 5000,
        responsive: [
            {
                breakpoint: 1280,
                settings: {
                    arrows: true,
                    slidesToShow: 3,
                    autoplay: true,
                    autoplaySpeed: 5000,
                }
            },
            {
                breakpoint: 600,
                settings: {
                    arrows: false,
                    slidesToShow: 2,
                    autoplay: true,
                    autoplaySpeed: 6000,
                }
            }
        ]
    });

    jQuery(".footer-products-slides.slick-slide.slick-active").each(function(index, e){
      jQuery(e).attr("data-active-slide-count", index);
      jQuery(e).addClass("footer-products-active-slide"+index);
   });

   jQuery('.footer-products-wrapper').on('afterChange', function( event, slick, add_custom_atts ){ 
      jQuery(".footer-products-slides.slick-slide.slick-active").each(function(index, e){
         jQuery(e).attr("data-active-slide-count", index);
         jQuery(".footer-products-slides.slick-slide.slick-active").removeClass("footer-products-active-slide"+index);
         jQuery(e).addClass("footer-products-active-slide"+index);
      });
   });
   
   jQuery('.animal-togglebtn').click(function(){
      var s = jQuery(this).parent().attr('id');
      var a = jQuery(this).parent().attr('data-toggle');
      var b = jQuery(this).attr('data-product');
      var c = '.' + b + '.'+ a;
      var d = '#maxime-lr-title.maxime-product-header-title.' + b;
      jQuery('.'+a).fadeOut();
      jQuery('.'+a).removeClass('active-row');
      jQuery(c).fadeIn();
      jQuery(c).addClass('active-row');
      jQuery('#maxime-lr-title.maxime-product-header-title').removeClass('active');
      jQuery(d).addClass('active');
      jQuery('#'+s+' '+'.animal-togglebtn').removeClass('active');
      jQuery(this).addClass('active');
      jQuery(this).parents('maxime-dog-prodsection-blug-bg').toggleClass('cat');
      console.log(s);
    });

   var maxHeight = 0;

    jQuery(".equalize").each(function(){
       if (jQuery(this).height() > maxHeight) { maxHeight = jQuery(this).height(); }
    });

    jQuery(".equalize").height(maxHeight);
});
